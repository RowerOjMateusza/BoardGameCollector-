<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.boardgamecollector">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BoardGameCollector"
        android:usesCleartextTraffic="true">
        <activity android:name=".BGG"></activity>
        <activity android:name=".History" />
        <activity android:name=".Dodatki" />
        <activity android:name=".LocationDetails" />
        <activity android:name=".AddLocation" />
        <activity android:name=".Locations"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Form" />
        <activity android:name=".form" />
        <activity android:name=".Details" />
        <activity android:name=".AddGame" />
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>